<%= render 'books/user_info', book: @book, user: @user %>

      <div class="col-xs-8">
      	<h2>Users</h2>
        	<table>
        		<tr>
        			<td class="user-img">image</td>
        			<td class="user-none"></td>
        			<td class="user-name">name</td>
        			<td class="user-show"></td>
              <td class="user-none"></td>
        		</tr>
        		<% @users.each do |f| %>
        		  <tr>
          			<td class="user-img">
                  <%= attachment_image_tag f, :user_image, size: '60x60', format: 'jpeg', fallback: "no_image.jpg",class: "user-img" %>
          			</td>
          			<td class="user-none">
                <!-- なし -->
          			</td>
          			<td class="user-name">
          				<%= f.name %>
          			</td>
                <td class="user-none">
                <!-- なし -->
                </td>
          			<td class="user-show">
          				<%= link_to "Show", user_path(f), class: "user_#{f.id}" %>
          			</td>
          		  </tr>
        		<% end %>
        	</table>
      </div>
  </div>
</div>